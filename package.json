{
  "name": "eleventy-blog",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "eleventy": "eleventy",
    "critical": "npx critical _site/index.html --inline --base _site/ --output _site/index.html --minify",
    "images": "npx eleventy-img src/img/* --output=_site/img/",
    "build": "npm run eleventy && npm run images && npm run critical"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.0",
    "html-minifier": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3"
  }
}
